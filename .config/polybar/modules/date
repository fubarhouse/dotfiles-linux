[module/date]
type = internal/date
interval = 5

date =
date-alt = " %Y-%m-%d"

time = %H:%M
time-alt = %H:%M:%S

format-padding = 1
format-prefix = "î•"
format-prefix-foreground = ${colors.foreground}
format-underline = ${colors.foreground}

label-foreground = ${colors.foreground}
label = %date% %time%